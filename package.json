	"main": "projects\\routine\\src\\public-api.ts",
	"scripts": {
		"ng": "ng",
		"build:supporting:routine": "gulp build:supporting:routine",
		"build:supporting:navigator": "gulp build:supporting:navigator",
		"build:routine-assistance": "ng build routine-assistance && yarn run build:supporting:routine",
		"build:module-navigator": "ng build module-navigator && yarn run build:supporting:navigator",
		"build:example:context": "ng build example-context",
		"build:example:module-a": "ng build example-module-a",
		"build:example:module-b": "ng build example-module-b",
		"build:example:app": "ng build example-app",
		"start:example:module-a": "ng serve example-module-a -o --port 3000",
		"start:example:module-b": "ng serve example-module-b -o --port 3001",
		"start:example:app": "ng serve example-app -o --port 5000",
		"start:example:navigator:all": "concurrently \"yarn start:example:module-a\" \"yarn start:example:module-b\" \"yarn start:example:app\" ",
		"start": "yarn start:example:navigator:all",
		"build:example-app": "ng build example-app",
		"serve:dist": "concurrently \"serve dist/example-app -l 5000 -s\" \"serve dist/example-module-a -l 3000 -s\" ",
		"build": "npm run build:example-app && npm run build:example:module-a",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
	},
